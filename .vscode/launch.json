{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Tauri Debug",
      "cargo": {
        "args": [
          "build",
          "--manifest-path=./src-tauri/Cargo.toml",
          "--no-default-features"
        ]
      },
      "preLaunchTask": "vite:dev",
      "sourceLanguages": ["rust"],
      "env": {
        "RUST_BACKTRACE": "1"
      }
    }
  ]
}
